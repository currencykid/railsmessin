<h1> Listing all articles </h1> 
<p> <%= link_to "Create new article", new_article_path %> </p>  
<table> 
	<tr>
		<th> Title </th> 
		<th> Description </th>
	</tr> 


	<% @articles.each do |x| %>
			
		<tr>
			<td> <%= x.title%> </td>
			<td> <%= x.description%> </td>
			<!-- <td> <%= x.created_at %> </td> --> 
			<td> <%= link_to "Edit" , edit_article_path(x) %> </td>
			<td> <%= link_to "Show" , article_path(x) %> </td>
		</tr> 
			
	
	<% end %> 

</table>
